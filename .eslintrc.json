{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "plugin:vue/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "jsx": false
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "rules": {
      "vue/name-property-casing": [
        "error",
        "PascalCase"
      ]
    },
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}
